{
  "name": "agents-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "policy:check": "node .github/scripts/enforce-agent-policies.mjs",
    "agent:sync": "node tools/bootstrap/agent-managed-files.mjs sync",
    "agent:drift:check": "node tools/bootstrap/agent-managed-files.mjs check",
    "agent:template-impact:check": "node tools/bootstrap/check-template-impact.mjs",
    "agent:preflight": "node .github/scripts/agent-session-preflight.mjs",
    "agent:payload:stub": "node .github/scripts/generate-subagent-payload-stub.mjs",
    "agent:queue:open": "node .github/scripts/agent-queue-lifecycle.mjs open-item",
    "agent:queue:start": "node .github/scripts/agent-queue-lifecycle.mjs start-item",
    "agent:queue:complete": "node .github/scripts/agent-queue-lifecycle.mjs complete-item",
    "agent:queue:close-feature": "node .github/scripts/agent-queue-lifecycle.mjs close-feature",
    "agent:queue:verify-evidence:check": "node .github/scripts/repair-queue-verification-evidence.mjs --check",
    "agent:queue:verify-evidence:write": "node .github/scripts/repair-queue-verification-evidence.mjs --write --run-cmd \"node .github/scripts/enforce-agent-policies.mjs\"",
    "index:build": "node tools/index/indexer.mjs build",
    "index:rebuild": "node tools/index/indexer.mjs build --full",
    "index:query": "node tools/index/indexer.mjs query",
    "index:verify": "node tools/index/indexer.mjs verify --strict",
    "feature-index:verify": "node tools/docs/feature-index-check.mjs --strict",
    "jsdoc-coverage:generate": "node tools/docs/generate-jsdoc-coverage.mjs --write",
    "jsdoc-coverage:verify": "node tools/docs/generate-jsdoc-coverage.mjs --check",
    "logging:compliance:generate": "node .github/scripts/verify-logging-compliance.mjs --write-baseline",
    "logging:compliance:verify": "node .github/scripts/verify-logging-compliance.mjs --check",
    "route-map:generate": "node tools/docs/generate-route-map.mjs --write",
    "route-map:verify": "node tools/docs/generate-route-map.mjs --check",
    "domain-readmes:generate": "node tools/docs/generate-domain-readmes.mjs --write",
    "domain-readmes:verify": "node tools/docs/generate-domain-readmes.mjs --check",
    "release:notes": "node .github/scripts/generate-release-notes.mjs",
    "release:prepare": "node .github/scripts/release-version-sync.mjs --write",
    "release:verify": "node .github/scripts/release-version-sync.mjs --check",
    "bootstrap:project": "node tools/bootstrap/bootstrap-project.mjs"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
